import csv


wind = list()

f = open('weather_a.csv', 'r', encoding='cp949')
rdr = csv.reader(f)

def max_wind_speed(a):

  max_speed = float(a[1])

  for i in a[1:]:
    
    if i == '':
      continue

    i = float(i)
    if max_speed < i:
      max_speed = i
   
  print("지난 10년간 울릉도의 최대 풍속은 ", max_speed, "m/s")

for row in rdr:
  wind.append(row[2])

max_wind_speed(wind)
f.close()
